{
  "name": "@story-brew/ai",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "build": "tsdown"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "dependencies": {
    "dotenv": "catalog:",
    "groq-sdk": "^0.34.0"
  },
  "devDependencies": {
    "@story-brew/typescript-config": "workspace:*",
    "tsdown": "catalog:",
    "typescript": "^5.0.0"
  }
}